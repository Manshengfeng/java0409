<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.offcn.dao.stuDao">
    <select id="getAll" resultType="com.offcn.po.NewStudent">
        SELECT * FROM newstudentinfo
    </select>
    
    <insert id="save" parameterType="com.offcn.po.NewStudent">
      insert into newstudentinfo(name,score,phone) values(#{name},#{score},#{phone})
    </insert>
</mapper>